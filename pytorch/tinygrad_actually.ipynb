   "source": [
    "from tinygrad.tensor import Tensor\n",
    "\n",
    "x = Tensor.eye(3)\n",
    "print(x._ctx)\n",
    "y = Tensor([[2.0,0,-2.0]])\n",
    "print(y._ctx)\n",
    "z = y.matmul(x).sum()\n",
    "print(z.__sub__)\n",
    "z.backward()\n",
    "print(z.__sub__)\n",
    "\n",
    "print(x.grad)  # dz/dx\n",
    "print(y.grad)  # dz/dy"
   ]
